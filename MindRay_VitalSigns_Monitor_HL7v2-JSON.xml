        <step>
          <sequenceNumber>3</sequenceNumber>
          <name>OBX Mapping</name>
          <script>var obxLength = msg[&apos;OBX&apos;].length();

vitalSignsData.VitalResults = [];

for(i=0;i&lt;obxLength;i++){

	var vitalObj = {};

	if(msg[&apos;OBX&apos;][i][&apos;OBX.2&apos;][&apos;OBX.2.1&apos;]==&apos;ST&apos;){
	vitalObj.valueType = &quot;String Data&quot;; 	
	}
	else if(msg[&apos;OBX&apos;][i][&apos;OBX.2&apos;][&apos;OBX.2.1&apos;]==&apos;CE&apos;){
	vitalObj.valueType = &quot;Coded Entry&quot;;	
	}
	else if(msg[&apos;OBX&apos;][i][&apos;OBX.2&apos;][&apos;OBX.2.1&apos;]==&apos;NM&apos;){
	vitalObj.valueType = &quot;Numeric&quot;;	
	}

	if(msg[&apos;OBX&apos;][i][&apos;OBX.3&apos;][&apos;OBX.3.2&apos;]!=&apos;&apos;){
	vitalObj.observationIdentityName = msg[&apos;OBX&apos;][i][&apos;OBX.3&apos;][&apos;OBX.3.2&apos;];	
	}
	
	vitalObj.observationIdentifier 	= msg[&apos;OBX&apos;][i][&apos;OBX.3&apos;][&apos;OBX.3.1&apos;];
	vitalObj.observationValue 		= msg[&apos;OBX&apos;][i][&apos;OBX.5&apos;][&apos;OBX.5.1&apos;];
	vitalObj.resultStatus 			= msg[&apos;OBX&apos;][i][&apos;OBX.11&apos;][&apos;OBX.11.1&apos;];

	vitalSignsData.VitalResults.push(vitalObj);
}</script>
